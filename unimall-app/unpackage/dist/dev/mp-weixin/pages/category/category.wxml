<view class="content"><view class="info-box"><view class="title">xxxxx店</view><view class="info">距离您xxkm</view><view class="switch"><view data-event-opts="{{[['tap',[['switchChange',[0]]]]]}}" class="{{['switchNums',deliverType==0?'currentNum':'']}}" bindtap="__e">{{selectOpen[0]}}</view><view data-event-opts="{{[['tap',[['switchChange',[1]]]]]}}" class="{{['switchNums',deliverType==1?'currentNum':'']}}" bindtap="__e">{{selectOpen[1]}}</view></view><input class="ser-input" type="text" value="搜索" disabled="{{true}}" data-event-opts="{{[['tap',[['navigateToPage',['/pages/product/search']]]]]}}" bindtap="__e"/></view><view class="container"><scroll-view class="left-aside" scroll-y="{{true}}"><block wx:for="{{flist}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['tabtap',['$0'],[[['flist','id',item.id]]]]]]]}}" class="{{['f-item','b-b',(item.id===currentId)?'active':'']}}" bindtap="__e">{{''+item.title+''}}</view></block></scroll-view><scroll-view class="right-aside" scroll-with-animation="{{true}}" scroll-y="{{true}}" scroll-top="{{tabScrollTop}}"><block wx:for="{{slist}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view class="s-list" id="{{'main-'+item.id}}"><block wx:for="{{item.childrenList}}" wx:for-item="titem" wx:for-index="__i2__" wx:key="id"><block wx:if="{{titem.parentId===item.id&&titem.goods.length>0}}"><view class="t-list"><text class="s-item title">{{item.title+" > "+titem.title}}</text><block wx:for="{{titem.goods}}" wx:for-item="good" wx:for-index="__i3__" wx:key="id"><view class="t-item"><image class="img" src="{{good.img+'?x-oss-process=style/200px'}}"></image><text class="title">{{good.title}}</text><text class="price">{{"￥ "+good.originalPrice/100}}</text><view class="chooseBtn">选规格</view></view></block></view></block></block></view></block></scroll-view></view></view>