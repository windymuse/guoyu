<view class="container"><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" autoplay="true" interval="3000" duration="500" circular="{{true}}" indicator-dots="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navigateToPage',['$0'],[[['carouselList','',index,'url']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.imgUrl}}"></image></swiper-item></block></swiper></view><view class="wm-section"><view data-event-opts="{{[['tap',[['navigateToPage',['/pages/category/category',1]]]]]}}" class="container" bindtap="__e"><view class="title">门店自取</view><view class="info">下单免排队</view><image class="img" src="https://guoyu-admin.windymuse.cn/static/img/%E9%97%A8%E5%BA%97%E8%87%AA%E5%8F%96.jpg" mode></image></view><view data-event-opts="{{[['tap',[['navigateToPage',['/pages/category/category',0]]]]]}}" class="container" bindtap="__e"><view class="title">外卖</view><view class="info">无接触配送，安全到家</view><image class="img" src="https://guoyu-admin.windymuse.cn/static/img/%E5%A4%96%E5%8D%96.jpg" mode></image></view></view><block wx:if="{{hasLogin}}"><view class="page-bottom"><view class="p-b-btn"><view class="title">{{"我的积分 "+userInfo.points}}</view><view class="info">积分可用于抵扣价格</view></view></view></block><block wx:if="{{windowSpuList&&windowSpuList.length>0}}"><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">今日推荐</text><text class="tit2">Shop Window</text></view></view></block><block wx:if="{{windowSpuList&&windowSpuList.length>0}}"><view class="group-section"><swiper class="g-swiper" duration="{{500}}"><block wx:for="{{windowSpuList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item hidden="{{!(index%2===0)}}" class="g-swiper-item"><view data-event-opts="{{[['tap',[['navToWindowSuggestSpu',[index]]]]]}}" class="g-item left" bindtap="__e"><image src="{{item.spuImg+'/400px'}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{''+item.spuTitle+''}}</text><view class="price-box"><text class="price">{{'￥'+(isVip?item.spuVipPrice/100.0+' [VIP]':item.spuPrice/100.0)+''}}</text><block wx:if="{{item.spuOriginalPrice>(isVip?item.spuVipPrice:item.spuPrice)}}"><text class="m-price">{{'￥'+item.spuOriginalPrice/100+''}}</text></block></view><view class="pro-box"><text>{{"累计销售:"+item.spuSales+"件"}}</text></view></view></view><block wx:if="{{index+1<windowSpuList.length}}"><view data-event-opts="{{[['tap',[['navToWindowSuggestSpu',[index+1]]]]]}}" class="g-item right" bindtap="__e"><image src="{{windowSpuList[index+1].spuImg}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{''+windowSpuList[index+1].spuTitle+''}}</text><view class="price-box"><text class="price">{{'￥'+(isVip?windowSpuList[index+1].spuVipPrice/100.0+' [VIP]':windowSpuList[index+1].spuPrice/100.0)+''}}</text><block wx:if="{{windowSpuList[index+1].spuOriginalPrice>(isVip?windowSpuList[index+1].spuVipPrice:windowSpuList[index+1].spuPrice)}}"><text class="m-price">{{'￥'+windowSpuList[index+1].spuOriginalPrice/100.0+''}}</text></block></view><view class="pro-box"><text>{{"累计销售:"+windowSpuList[index+1].spuSales+"件"}}</text></view></view></view></block><block wx:if="{{index+1===windowSpuList.length}}"><view class="g-item right"></view></block></swiper-item></block></swiper></view></block><block wx:if="{{categoryPickList.length>0}}"><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">分类精选</text><text class="tit2">Competitive Products For You</text></view></view></block><block wx:for="{{categoryPickList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{categoryPickList.length>0}}"><view class="hot-floor"><view class="floor-img-box"><image class="floor-img" src="{{item.imgUrl}}" mode="scaleToFill"></image></view><scroll-view class="floor-list" scroll-x="{{true}}"><view class="scoll-wrapper"><block wx:for="{{item.data}}" wx:for-item="spuItem" wx:for-index="skuIndex" wx:key="skuIndex"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['categoryPickList','',index],['data','',skuIndex,'id']]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{spuItem.img+'/200px'}}" mode="aspectFill"></image><text class="title clamp">{{spuItem.title}}</text><text class="price">{{"￥"+(isVip?spuItem.vipPrice:spuItem.price)/100}}</text></view></block><view data-event-opts="{{[['tap',[['naviageToPage',['$0'],[[['categoryPickList','',index,'url']]]]]]]}}" class="more" bindtap="__e"><text>查看全部</text><text>More+</text></view></view></scroll-view></view></block></block><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">热销宝贝</text><text class="tit2">Hot Sale</text></view></view><view class="guess-section"><block wx:for="{{salesTop}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['salesTop','',index,'id']]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.img+'/400px'}}" mode="aspectFill"></image></view><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+(isVip?item.vipPrice/100+' [VIP]':item.price/100)}}</text></view></block></view></view>